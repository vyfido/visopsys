BUILDDIR = ../../build/programs

all:
	${MAKE} -C fontutil.dir
	${MAKE} -C helpfiles
	${MAKE} -C mines.dir
	${MAKE} -C snake.dir

clean:
	${MAKE} -C fontutil.dir clean
	${MAKE} -C helpfiles clean
	${MAKE} -C mines.dir clean
	${MAKE} -C snake.dir clean
	rm -f *~ core

