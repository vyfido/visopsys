msgid "OK"
msgstr "ОК"

#: format.c:221 format.c:288 format.c:314
msgid "Cancel"
msgstr "Отмена"

#: format.c:281
#, c-format
msgid ""
"The disk is mounted as %s.  It is STRONGLY recommended\n"
"that you unmount before continuing"
msgstr ""
"Диск подключен как %s.  НАСТОЯТЕЛЬНО рекомендуем\n"
"отключить его перед тем как продолжить"

#: format.c:287
msgid "Disk is mounted"
msgstr "Диск подключен"

#: format.c:288 format.c:302
msgid "Ignore"
msgstr "Игнорировать"

#: format.c:288 format.c:308
msgid "Unmount"
msgstr "Отключить"

#: format.c:293
#, c-format
msgid ""
"\n"
"%s (I)gnore/(U)nmount/(C)ancel?: "
msgstr ""
"\n"
"%s (I)игнорировать/(U)отключить/(C)отмена?: "

#: format.c:334
#, c-format
msgid "Unable to unmount %s"
msgstr "Невозможно отключить %s"

#: format.c:367
#, c-format
