#, c-format
msgid "Error %d copying boot sector \"%s\" to disk %s"
msgstr "Ошибка  %d копирования загрузочного сектора \"%s\" to disk %s"

#: format.c:391
msgid "usage:\n"
msgstr "использование:\n"

#: format.c:392
#, c-format
msgid "%s [-l] [-s] [-t type] [-T] [disk_name]\n"
msgstr "%s [-l] [-s] [-t тип] [-T] [имя_диска]\n"

#: format.c:443
msgid "Missing volume name argument for '-n' option"
msgstr "Пропущен аргумент 'Метка тома' для опции -n"

#: format.c:460
msgid "Missing type argument for '-t' option"
msgstr "Пропущен аргумент 'Тип' для опции -t"

#: format.c:473
#, c-format
msgid "Missing parameter for %s option"
msgstr "Пропущен параметр для опции %s"

